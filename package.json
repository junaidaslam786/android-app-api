{
  "name": "androidapp-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f backend",
    "docker:build": "docker-compose build",
    "docker:rebuild": "docker-compose build --no-cache",
    "docker:clean": "docker-compose down -v",
    "docker:restart": "docker-compose restart backend",
    "docker:ps": "docker-compose ps",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:docker": "docker-compose exec backend npm run typeorm --",
    "build:docker": "docker-compose exec backend npm run build",
    "migration:generate": "npm run typeorm -- migration:generate ./migrations/Init -d ./src/config/data-source.ts",
    "migration:run": "docker-compose exec backend npm run build && docker-compose exec backend npm run typeorm -- migration:run -d ./dist/config/data-source.js",
    "migration:revert": "docker-compose exec backend npm run typeorm -- migration:revert -d ./dist/config/data-source.js",
    "migration:show": "docker-compose exec backend npm run typeorm -- migration:show -d ./dist/config/data-source.js",
    "seed": "ts-node ./seeds/seed.ts",
    "seed:phase1": "docker-compose exec backend npm run seed:phase1:local",
    "seed:phase1:local": "ts-node seeds/phase1-seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@supabase/supabase-js": "^2.56.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.19",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^6.0.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-ts-node-esm": "^0.3.20",
    "typescript": "^5.3.3"
  }
}
